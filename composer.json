{
    "name": "mlab/budgetcontrol",
    "description": "Simple web site template base",
    "type": "metapackage",
    "repositories": {
        "mlabfactory/libs-mailer": {
            "type": "vcs",
            "url": "https://github.com/mlabfactory/Libs_Mailer.git"
        },
        "mlabfactory/wordpress-php-sdk": {
            "type": "vcs",
            "url": "https://github.com/mlabfactory/WordPress-PHP-SDK.git"
        }
    },
    "require": {
        "illuminate/database": "^10.43",
        "monolog/monolog": "^3.5",
        "respect/validation": "^2.3",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6",
        "twig/twig": "^3.8",
        "slim/slim": "^4.0",
        "robmorgan/phinx": "^0.16.0",
        "mlabfactory/libs-mailer": "^1.1",
        "mlabfactory/wordpress-php-sdk": "dev-master",
        "illuminate/cache": "^10.48",
        "illuminate/filesystem": "^10.48"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Mlab\\BudetControl\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Marco De Felice",
            "email": "marco.defelice@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
